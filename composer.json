{
    "name": "koolserve/cloudflare",
    "description": "A bolt extension to show analytics from Cloudflare about your website.",
    "type": "bolt-extension",
    "keywords": [
        "Cloudflare"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Chris Hilsdon",
            "email": "chris@koolserve.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Koolserve\\Cloudflare\\": "src",
            "Cloudflare\\": "lib/alpharecon19/cloudflare-php/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Koolserve\\Cloudflare\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Koolserve\\Cloudflare\\CloudflareExtension",
        "bolt-icon": "icon.png"
    }
}
